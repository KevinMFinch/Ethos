    <section id="completed">
        <div class="container">
            <div class="panel panel-default">
              <!-- Default panel contents -->
              <div class="panel-heading">
                <%= doc.category %>
                <small>Completed</small></div>
                <% var i = 0 %>
                <% doc.completed.forEach(function(item){ %>
                  <ul class="list-group">
                    <li class="list-group-item">
                        <%= item %>
                        <div class="dropdown" style="float: right; bottom: 7px">
                          <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                            <span class="caret"></span>
                          </button>
                          <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenu1">
                            <li><a href="#">Move to Current</a></li>
                            <li><a href="#">Move to Planned</a></li>
                            <li><a href="#">Edit</a></li>
                            <li role="separator" class="divider"></li>
                            <form action=<%= "/lists/" + doc.category.split(" ").join("") + "/completed/delete" %> method="POST">
                              <input type="hidden" name="index" value=<%= i %>>
                              <li><button class="remove" type="submit">Remove</button></li>
                            </form>
                          </ul>
                        </div>
                    </li>
                  </ul>
                  <% i++ %>
                <% }); %>
                <li class="list-group-item">
                    <form action=<%= "/lists/" + doc.category.split(" ").join("") + "/completed" %> method="POST">
                        <div class="row">
                          <div class="col-lg-6">
                            <div class="input-group">
                                <input type="text" class="form-control" name="item" placeholder="Item">
                                <span class="input-group-btn">
                                  <button class="btn btn-primary" type="submit">Add</button>
                                </span>
                            </div><!-- /input-group -->
                          </div><!-- /.col-lg-6 -->
                        </div><!-- /.row -->
                    </form>
                </li>
            </div>
        </div>
    </section>

