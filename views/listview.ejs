    <div class="container">
        <div class="page-header">
            <h1>What You Have, What You Want <small>Books made easy</small></h1>
        </div>
    </div>

    <section id="complete-book">
        <div class="container">
            <div class="panel panel-default">
              <!-- Default panel contents -->
              <div class="panel-heading">
            <% var doc = docs[0]; %>
                <%= doc.category %>
                <small>Completed</small></div>

              <ol class="list-group">
                <li class="list-group-item">
                    <% doc.completed.forEach(function(item){ %>
                        <%= item %>
                    <% }); %>
                    <div class="dropdown" style="float: right; bottom: 7px">
                      <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                        <span class="caret"></span>
                      </button>
                      <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                        <li><a href="#">Move to Current</a></li>
                        <li><a href="#">Move to Planned</a></li>
                        <li><a href="#">Edit</a></li>
                      </ul>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="row">
                      <div class="col-lg-6">
                        <div class="input-group">
                          <input type="text" class="form-control" placeholder="Item">
                          <span class="input-group-btn">
                            <button class="btn btn-primary" type="submit">Add</button>
                          </span>
                        </div><!-- /input-group -->
                      </div><!-- /.col-lg-6 -->
                    </div><!-- /.row -->
                </li>
              </ol>
            </div>
        </div>
    </section>

